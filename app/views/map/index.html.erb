<div id="map"></div>
    <script>
      const key = '<%= Rails.application.credentials.maptiler_api_key! %>';
      const map = L.map('map').setView([51.507579, -0.122974], 12); //starting position
      const mtLayer = L.maptiler.maptilerLayer({
        apiKey: key,
        style: L.maptiler.MapStyle.STREETS, // optional
      }).addTo(map);
    </script>
